{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "allow-return-shorthand": false,
    "arrow-parens": false,
    "interface-name": false,
    "jsx-no-lambda": false,
    "member-ordering": false,
    "no-console": false,
    "no-empty": false,
    "no-unused-expression": true,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "variable-name": false
  }
}
